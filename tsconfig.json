{
    "compilerOptions": {
        "module": "es6",
		"jsx": "react",
        "target": "es6",
        "alwaysStrict": false,
		"noImplicitAny": false,
		"skipLibCheck": true,
        "strictNullChecks": true,
        "allowJs": true,
		"isolatedModules": true,
		"noEmit": true
    },
    "include": [
        "src/**/*",
		"loader.js",
		"sandbox.js"
    ]
}
{
    "name": "@alixlahuec/zotero-roam",
    "version": "0.7.10",
    "description": "Connector to Zotero for Roam Research users",
    "license": "Apache-2.0",
    "author": {
        "name": "Alix Lahuec",
        "email": "alix.lahuec@onmail.com",
        "url": "https://ae-lhc.com"
    },
    "repository": "alixlahuec/zotero-roam",
    "bugs": {
        "url": "https://github.com/alixlahuec/zotero-roam/issues",
        "email": "alix.lahuec@onmail.com"
    },
    "main": "src/index.js",
    "browser": "dist/zoteroRoam.min.js",
    "unpkg": "dist/zoteroRoam.min.js",
    "msw": {
        "workerDirectory": "public"
    },
    "scripts": {
        "build:dev": "webpack --config dev/webpack.dev.config.js",
        "build:prod": "webpack --config dev/webpack.config.js",
        "build:roam": "webpack --config dev/webpack.roam.config.js",
        "build:dev-roam": "webpack --config dev/webpack.dev.roam.config.js",
        "build:sandbox": "webpack --config dev/webpack.sandbox.config.js",
        "lint": "eslint loader.js sandbox.js .storybook/** src/** stories/** tests/**",
        "lint:fix": "npm run lint -- --fix",
        "ci:lint": "npm run lint -- --format junit -o reports/eslint-junit.xml",
        "test": "jest",
        "ci:test": "jest --ci --collectCoverage",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook --quiet",
        "test-storybook": "test-storybook",
        "ci:test-storybook": "test-storybook --coverage --junit",
        "version-release": "release-it --config dev/.release-it.json --ci --no-git.tag --no-npm.publish --no-github.release",
        "version-release:latest": "npm run version-release",
        "version-release:beta": "npm run version-release -- --preRelease=beta",
        "publish-release": "release-it --config dev/.release-it.json --ci --npm.skipChecks --no-increment",
        "publish-release:latest": "npm run publish-release -- --git.requireBranch=main --github.autoGenerate",
        "publish-release:beta": "npm run publish-release -- --git.requireBranch=beta --preRelease=beta --github.releaseNotes=\"echo\"",
        "release:preview": "release-it --config dev/.release-it.json --dry-run"
    },
    "sideEffects": [
        "*.css",
        "src/index.js"
    ],
    "babel": {
        "presets": [
            "@babel/preset-env",
            [
                "@babel/preset-react",
                {
                    "runtime": "automatic"
                }
            ]
        ]
    },
    "jest-junit": {
        "includeConsoleOutput": true,
        "outputDirectory": "reports",
        "outputName": "storybook-junit.xml"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@storybook/addon-a11y": "^6.5.14",
        "@storybook/addon-actions": "^6.5.14",
        "@storybook/addon-controls": "^6.5.14",
        "@storybook/addon-coverage": "^0.0.7",
        "@storybook/addon-essentials": "^6.5.14",
        "@storybook/addon-interactions": "^6.5.14",
        "@storybook/addon-links": "^6.5.14",
        "@storybook/builder-webpack5": "^6.5.14",
        "@storybook/cli": "^6.5.14",
        "@storybook/jest": "^0.0.10",
        "@storybook/manager-webpack5": "^6.5.14",
        "@storybook/react": "^6.5.14",
        "@storybook/test-runner": "^0.9.3",
        "@storybook/testing-library": "^0.0.13",
        "@testing-library/react": "^12.1.5",
        "@testing-library/react-hooks": "^8.0.1",
        "auto-changelog": "^2.4.0",
        "axe-playwright": "^1.1.11",
        "babel-jest": "^28.1.3",
        "babel-loader": "^9.1.2",
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^4.1.0",
        "eslint": "^8.24.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^27.0.4",
        "eslint-plugin-react": "^7.31.8",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.10",
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^28.1.3",
        "jest-junit": "^15.0.0",
        "jest-transform-css": "^6.0.1",
        "mini-css-extract-plugin": "^2.7.2",
        "msw": "^0.47.3",
        "msw-storybook-addon": "^1.6.3",
        "release-it": "^15.5.1",
        "style-loader": "^3.3.1",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@blueprintjs/core": "^3.50.4",
        "@blueprintjs/datetime": "^3.23.14",
        "@blueprintjs/icons": "^3.33.0",
        "@blueprintjs/popover2": "^0.14.0",
        "@blueprintjs/select": "^3.18.6",
        "@tanstack/react-query": "^4.7.1",
        "axios": "^0.27.2",
        "axios-retry": "^3.3.1",
        "prop-types": "^15.8.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "scite-badge": "github:scitedotai/scite-badge#v8.0.6-prod",
        "tributejs": "^5.1.3"
    }
}

{
    "name": "@alixlahuec/zotero-roam",
    "version": "0.6.99-beta.2",
    "description": "Connector to Zotero for Roam Research users",
    "license": "Apache-2.0",
    "author": {
        "name": "Alix Lahuec",
        "email": "alix.lahuec@onmail.com",
        "url": "https://ae-lhc.com"
    },
    "repository": "alixlahuec/zotero-roam",
    "bugs": {
        "url": "https://github.com/alixlahuec/zotero-roam/issues",
        "email": "alix.lahuec@mail.mcgill.ca"
    },
    "main": "src/index.js",
    "browser": "dist/zoteroRoam.min.js",
    "unpkg": "dist/zoteroRoam.min.js",
    "msw": {
        "workerDirectory": "public"
    },
    "scripts": {
        "build:dev": "webpack --config webpack.dev.config.js",
        "build:prod": "webpack",
        "build:roam": "webpack --config webpack.roam.config.js",
		"build:sandbox": "webpack --config webpack.sandbox.config.js",
        "lint": "eslint --fix",
        "ci:lint": "eslint src/**",
        "test": "jest",
        "ci:test": "jest --ci --collectCoverage",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook --quiet",
        "test-storybook": "test-storybook",
        "ci:test-storybook": "test-storybook --coverage",
        "release:latest": "release-it --ci --git.requireBranch=main --github.autoGenerate --npm.skipChecks",
        "release:beta": "release-it --ci --git.requireBranch=beta --preRelease=beta --github.releaseNotes=\"echo\" --npm.skipChecks",
        "release:preview": "release-it --dry-run"
    },
    "sideEffects": [
        "*.css",
        "src/index.js"
    ],
    "devDependencies": {
        "@babel/core": "^7.18.13",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@storybook/addon-a11y": "^6.5.10",
        "@storybook/addon-actions": "^6.5.10",
        "@storybook/addon-controls": "^6.5.10",
        "@storybook/addon-coverage": "^0.0.4",
        "@storybook/addon-essentials": "^6.5.10",
        "@storybook/addon-interactions": "^6.5.10",
        "@storybook/addon-links": "^6.5.10",
        "@storybook/builder-webpack5": "^6.5.10",
        "@storybook/cli": "^6.5.10",
        "@storybook/jest": "^0.0.10",
        "@storybook/manager-webpack5": "^6.5.10",
        "@storybook/react": "^6.5.10",
        "@storybook/test-runner": "^0.7.0",
        "@storybook/testing-library": "^0.0.13",
        "@testing-library/react": "^12.1.5",
        "@testing-library/react-hooks": "^8.0.1",
        "auto-changelog": "^2.4.0",
        "axe-playwright": "^1.1.11",
        "babel-jest": "^28.1.3",
        "babel-loader": "^8.2.5",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.0.0",
        "eslint": "^8.23.0",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.9.0",
        "eslint-plugin-react": "^7.31.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.4",
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^28.1.3",
        "jest-transform-css": "^4.0.1",
        "mini-css-extract-plugin": "^2.6.1",
        "msw": "^0.46.0",
        "msw-storybook-addon": "^1.6.3",
        "release-it": "^15.4.1",
        "style-loader": "^3.3.1",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@blueprintjs/core": "^3.50.4",
        "@blueprintjs/datetime": "^3.23.14",
        "@blueprintjs/icons": "^3.33.0",
        "@blueprintjs/popover2": "^0.14.0",
        "@blueprintjs/select": "^3.18.6",
        "@sentry/react": "^7.12.0",
        "@sentry/tracing": "^7.12.0",
        "@tanstack/react-query": "^4.2.3",
        "axios": "^0.27.2",
        "axios-retry": "^3.3.1",
        "prop-types": "^15.8.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "scite-badge": "github:scitedotai/scite-badge#v8.0.6-prod",
        "tributejs": "^5.1.3"
    }
}
